{
  "files": ["generateDocs.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2021",
    "lib": [
      "es6",
      "dom", 
      "es2017",
      "es2021",
    ],
    "strict": true,
    "module": "commonjs",
    "resolveJsonModule": true, 
    "rootDir": "./", 
    "outDir": "dist",
    "noImplicitAny": false, 
    "declaration": true,
    "strictNullChecks": true,
    "declarationMap": true,
    "ignoreDeprecations": "5.0", 
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
  }, 
  "exclude": [ 
    "node_modules"
  ]
}