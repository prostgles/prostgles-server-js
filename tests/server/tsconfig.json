{
	"files": ["./index.ts"],
	"compilerOptions": {
		"target": "es2022",
		"lib": [ "es2022" ],
		"esModuleInterop" : true,
		"allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
		"allowJs": true,
		"module": "commonjs",
    "moduleResolution": "node",
		"declaration": true,
		"declarationMap": true,
		"keyofStringsOnly": true
	},
	"exclude": [
		"dist",
		"DBoGenerated.ts"
	]
}